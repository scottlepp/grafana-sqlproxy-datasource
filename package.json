  "devDependencies": {
    "@grafana/data": "7.1.3",
    "@grafana/runtime": "7.1.3",
    "@grafana/toolkit": "7.1.3",
    "@grafana/ui": "7.1.3",
    "@types/lodash": "4.14.123",
    "@types/react": "16.8.16 ",
    "@types/request-promise-native": "^1.0.17",
    "node-fetch": "2.6.1",
    "@types/node-fetch": "2.5.7",
    "@grafana/tsbackend": "srclosson/grafana-tsbackend#master"
  },
  "screenshots": [],
  "dependencies": {
    "codemirror": "^5.51.0",
    "date-fns": "^2.16.1",
    "react-codemirror2": "^6.0.0"
  }
